                <SetRaster raster="4" />
                <Wait duration="30" />
                <SetRaster raster="5" />
                <Wait duration="16" />
                <Goto pos="4" />
            </Component>
        </Animation>
        <Animation name="Panic">
            <Component name="Comp_0" xyz="0,0,0">
                <SetRaster raster="6" />
