            </Component>
        </Animation>
        <Animation name="Hurt">
            <Component name="Comp_0" xyz="0,0,0">
                <SetRot xyz="0,0,10" />
                <SetRaster raster="9" />
                <Wait duration="6" />
                <SetRot xyz="0,0,10" />
                <SetRaster raster="A" />
                <Wait duration="6" />
                <Goto pos="0" />
            </Component>
        </Animation>
        <Animation name="Back">
            <Component name="Comp_0" xyz="0,0,0">
                <SetRaster raster="B" />
                <Wait duration="8" />
                <SetRaster raster="C" />
                <Wait duration="8" />
                <Goto pos="0" />
            </Component>
        </Animation>
        <Animation name="Talk">
            <Component name="Comp_0" xyz="0,0,0">
                <SetRaster raster="D" />
                <Wait duration="8" />
                <SetRaster raster="0" />
                <Wait duration="8" />
                <Goto pos="0" />
            </Component>
        </Animation>
    </AnimationList>
