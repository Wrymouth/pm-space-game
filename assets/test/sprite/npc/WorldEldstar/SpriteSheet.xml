            </Component>
        </Animation>
        <Animation name="Idle">
            <Component name="Comp_0" xyz="0,-8,0">
                <SetRaster raster="0" />
                <Wait duration="14" />
                <SetRaster raster="1" />
                <Wait duration="14" />
