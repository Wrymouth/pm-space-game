                <SetRaster raster="3" />
                <Wait duration="10" />
                <Goto pos="0" />
            </Component>
        </Animation>
        <Animation name="Leap">
            <Component name="Comp_0" xyz="0,0,0">
                <SetRaster raster="0" />
