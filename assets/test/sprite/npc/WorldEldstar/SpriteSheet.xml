        <Palette id="0" src="Default.png" />
        <Palette id="1" src="Glowing.png" />
    </PaletteList>
    <RasterList>
        <Raster id="0" palette="0" src="Idle1.png" />
        <Raster id="1" palette="0" src="Idle2.png" />
        <Raster id="2" palette="0" src="Wave1.png" />
        <Raster id="3" palette="0" src="Wave2.png" />
        <Raster id="4" palette="0" src="Leap1.png" />
        <Raster id="5" palette="0" src="Leap2.png" />
